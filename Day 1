import java.util.*;
public class Day1 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        float a = sc.nextFloat();
        float b = sc.nextFloat();
        float c = sc.nextFloat();
        float Avg = (float)(a + b + c)/3;
        System.out.println("Average of 3 Numbers is " + Avg + " .");
    }
}
